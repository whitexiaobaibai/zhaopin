<script type="text/javascript">
    function selectIndex(ind) {
        var el = this;
        el.find('ul li').eq(ind).addClass("active").siblings().removeClass('active');
        el.find('.j_switch > div').eq(ind).show().siblings().hide();
    }
    function OpenLoginOrRegister(backurl) {
        if (backurl != null) {
            $("#backurl").val(backurl);
        }
        else {
            backurl = GetCookie("cdcbackurl");
            if (backurl == null || backurl == "") {
                SetCookie("cdcbackurl", document.URL);
            }
        }
        //读取验证码
        $("#vimg").attr("src", "//xiaoyuan.zhaopin.com/LoginManager/GetValidateCode?r=" + Math.random());
        var popdiv = $("#newWin");
        wbLogin.param.popLayer = 0;
        if(arguments[1]) {
            popdiv = $("#newWin2");
            wbLogin.param.popLayer = 2;
        }
        selectIndex.call(popdiv, 0);
        $.popupBase.init({ div: popdiv, posx: "center", posy: "center", maskClose: false });
        wbLogin.Common.EnterEvent(function () {
            $("#login").click();
        });
        return false;
    }
    
    function IsActivied(fn,ele) {
        $.ajax({
            url: "/PublicModule/EmailActived?t=" + (new Date()).getTime(),
            dataType: 'json',
            async: false, //同步
            success: function (data) {
                if (!data.status) {
                    wbLogin.Doms.init();
                    $.popupBase.close(wbLogin.param.popLayer ? wbLogin.Doms.Register : wbLogin.Doms.Login);
                    wbLogin.Events.mailAlert(null, data.email);
                    return;
                }
                if (typeof fn === "function")
                    fn.call(ele ? ele : null, null);
            }
        });
    }

    function reset() {
        wbLogin.Events.popReset();
    }
    $(function () {
        wbLogin.param.popBase = true;
        wbLogin.Doms.init();
        wbLogin.Events.init();
        $(".nL_close,.j_ln_closeBtn").click(function () {
            DelCookie("cdcbackurl");
            wbLogin.Events.InputClean(); ///
            $.popupBase.close($(this).closest(".nL_main"));
        });
        $("a[name='bindcancel']").click(function () {
            $.popupBase.close($("#otherbind"));
            OpenLoginOrRegister();
            return false;
        });
        $("#accept").click(function () {
            $("#devRegister").removeClass(this.checked ? "btn-visited" : "btn-success_new").addClass(this.checked ? "btn-success_new" : "btn-visited");
        });

        $(".regforlogin").each(function (i) {
            $(this).click(function () {
                wbLogin.param.popLayer = 0;
                $.popupBase.close($("#newWin2"));
                selectIndex.call($("#newWin"), i);
                $.popupBase.init({ div: $("#newWin"), posx: "center", posy: "center", maskClose: false });
                return false;
            });
        });

        //$(".loginforreg").each(function (i) {
        //    $(this).click(function () {
        //        wbLogin.param.popLayer = 2;
        //        $.popupBase.close($("#newWin"));
        //        selectIndex.call($("#newWin2"), i);
        //        $.popupBase.init({ div: $("#newWin2"), posx: "center", posy: "center", maskClose: false });
        //        return false;
        //    });
        //});

        var pw = $("input[type='password']");
        pw.each(function () {
            passwordInputPlaceholder($(this), "输入密码");
        });

    });
</script>


    
