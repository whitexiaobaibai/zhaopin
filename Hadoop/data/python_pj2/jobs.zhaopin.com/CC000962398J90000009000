        <script type="text/javascript">
            $(function () {
                var cdc_currentChild,
                cdc_currentSub = "f_7_0";
                
    cdc_currentChild = 'f_1';

                var curobj = $("#" + cdc_currentChild),
                    cursub = $("#" + cdc_currentSub);
                //var tmp = String("False").toLowerCase() == "true" ? true : false;
                if (curobj.length > 0) {
                    curobj.siblings('li').removeClass('hover').children("a").removeClass("hover");
                    curobj.addClass("hover").children('a').addClass('hover');
                }
                if (cursub.length > 0) {
                    cursub.addClass('current').siblings().removeClass('current')
                }


                //头部菜单
                $(".hasMenu").hover(function () {
                    $(this).addClass("cur");
                    $(this).find(".cIcon").removeClass("cIcon_DownArrowIndex").addClass("cIcon_UpArrowIndex")
                    $(this).find(".subMenu").show();
                }, function () {
                    $(this).removeClass("cur");
                    $(this).find(".cIcon").removeClass("cIcon_UpArrowIndex").addClass("cIcon_DownArrowIndex")
                    $(this).find(".subMenu").hide();
                });

                //收藏按钮
                $(".collectBtn").click(function () {
                    alert("请按Ctrl/Cmd+D收藏本站，好工作不用愁！")
                });
            });
            $(function () {
                $("#hreftopreg").click(function () {
                    try {
                        window.zhuge.track("RegEnter", { 'enter': 'top', 'locaUrl': window.location.href });
                    } catch (e) {
                    }
                });
                $("#hrefWinReg").click(function () {
                    try {
                        window.zhuge.track("RegEnter", { 'enter': 'NewWin', 'locaUrl': window.location.href });
                    } catch (e) {
                    }
                });
                $(".navBox li").click(function () {
                    try {
                        window.zhuge.track('MainNavBar', {
                            localUrl: window.location.href,
                            pagemenuclick: $.trim($(this).text())
                        });
                    } catch (e) {
                    }
                });
            });
        </script>
